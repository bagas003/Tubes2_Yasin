<Window x:Class="Tubes2_Yasin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Try.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TextBlock HorizontalAlignment="Center" Height="39" Margin="0,7,0,0" TextWrapping="Wrap" Text="Treasure Hunt Solver" VerticalAlignment="Top" Width="288" FontSize="30"/>
        <Rectangle Stroke="Black" Margin="50,47,50,382" RenderTransformOrigin="0.5,0.5">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>
        <Rectangle Stroke="Black" Margin="234,231,245,202" RenderTransformOrigin="0.5,0.5">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90.022"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>
        <TextBlock HorizontalAlignment="Left" Height="22" Margin="50,88,0,0" TextWrapping="Wrap" Text="Input" VerticalAlignment="Top" Width="56" FontWeight="Bold" FontSize="17" RenderTransformOrigin="0.161,1.306"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="422,88,0,0" TextWrapping="Wrap" Text="Output" VerticalAlignment="Top" Width="63" FontWeight="Bold" FontSize="17" RenderTransformOrigin="3.518,1.132"/>
        <Button Click="Button_Click" Width="100" Height="30" Content="Select File" Margin="50,122,650,282"/>
        <TextBlock HorizontalAlignment="Left" Margin="50,173,0,0" TextWrapping="Wrap" Text="Algoritma" VerticalAlignment="Top" FontSize="13"/>
        <TextBlock HorizontalAlignment="Left" Margin="50,242,0,0" TextWrapping="Wrap" Text="Algoritma" VerticalAlignment="Top" FontSize="13"/>
        <RadioButton Content="BFS" HorizontalAlignment="Left" Margin="50,195,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,0.671"/>
        <RadioButton Content="DFS" HorizontalAlignment="Left" Margin="50,217,0,0" VerticalAlignment="Top"/>
        <RadioButton Content="Biasa" HorizontalAlignment="Left" Margin="50,264,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,0.671" Checked="RadioButton_Checked_1"/>
        <RadioButton Content="TSP" HorizontalAlignment="Left" Margin="50,284,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,0.671" Checked="RadioButton_Checked_1"/>
        <Button Margin="122,323,545,75" Click="Button_Click_2">
            <Button Content="Visualize" Click="Button_Click_1" Width="68"/>
        </Button>
        <Grid Margin="449,117,50,117" Grid.RowSpan="2" Name="myMap"/>
    </Grid>
</Window>
